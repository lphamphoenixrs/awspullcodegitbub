<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ErrorLevel">
	<resultMap id="ErrorLevelMap"
		type="com.phoenixrs.api.entities.ErrorLevelEntity">
		<result property="id" column="id" />
		<result property="name" column="name" />
		<result property="text" column="text" />
		<result property="value" column="value" />
		<result property="offset" column="offset" />
		<result property="status" column="status" />
		<result property="is_delete" column="is_delete" />
		<result property="created_date" column="created_date" />
		<result property="created_by" column="created_by" />
		<result property="updated_date" column="updated_by" />
		<result property="active" column="active" />
	</resultMap>
	

	

	<select id="getList" resultMap="ErrorLevelMap" parameterType="String">
		SELECT
			t.*,
			t.name AS text,
			0 as active
		FROM
			error_level AS t
		WHERE
			t.`status` = 1 AND t.is_delete = 0 
		ORDER BY `name` ASC;
	</select>
	
</mapper>